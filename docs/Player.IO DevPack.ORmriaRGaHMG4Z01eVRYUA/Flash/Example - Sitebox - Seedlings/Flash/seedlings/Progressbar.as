package seedlings{	import flash.display.MovieClip;	import flash.display.Sprite;		//Progressbar class	public class Progressbar extends MovieClip{		public function Progressbar() {			setProgress(0);			//Prevent bar from being clicked			this.mouseEnabled = false;			this.mouseChildren = false;		}		public function setProgress(progress:Number):void{			//Set progress			msk.scaleX = progress		}	}}