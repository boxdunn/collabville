package seedlings{	import flash.display.MovieClip;	import flash.utils.setInterval;		//Class that controls flower faces	public class Face extends MovieClip{		public function Face() {			randomLook();			//Change face at a random interval between 10 and 20 secs			setInterval(randomLook, Math.random()*10000+10000);		}				private function randomLook():void{			//Set random face between 1 and 4			gotoAndStop((Math.random()*3>>0)+1)		}	}	}