package seedlings{	import flash.display.MovieClip;	import flash.events.Event;		//Background sky handler	public class Sky extends MovieClip{		public function Sky() {			addEventListener(Event.ENTER_FRAME, handleEnterFrame);		}		private function handleEnterFrame(e:Event):void{			//Move sky depended on size, giving the illution of perspective			this.x += 0.005 + (this.width/1000)			if(this.x > this.width + stage.stageWidth) this.x = -this.width		}	}}