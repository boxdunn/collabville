package states{	import flash.display.MovieClip;	import flash.events.MouseEvent;	import playerio.PlayerIO;	import states.introstate.HighscoreEntry;	import playerio.DatabaseObject;	public class IntroState extends State{		public function IntroState() {			btnstartgame.addEventListener(MouseEvent.MOUSE_DOWN, handleStartGame)			currentuser.text = "Authed as " + LoginState.username + " you are at level " + (LoginState.playerObject.level || "0")						LoginState.client.bigDB.loadRange("PlayerObjects", "level", [], null, null, 10, handleGetToplist)					}		private function handleGetToplist(list:Array):void{			list.sortOn( ["level"], Array.NUMERIC | Array.DESCENDING);			for( var a:int=0;a<list.length;a++){				var co:DatabaseObject = list[a] as DatabaseObject								var r:HighscoreEntry = new HighscoreEntry( a+1, co.key, parseInt(co.level+1) );				addChild(r)				r.x = 152				r.y = 126 + a * 15							}		}		private function handleStartGame(e:MouseEvent):void{			base.startGame();			e.preventDefault()			e.stopImmediatePropagation()		}	}}